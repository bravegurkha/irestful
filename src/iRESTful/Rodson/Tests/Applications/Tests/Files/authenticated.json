        "controllers": {
            "retrieve_partial_set": {
                "pattern": "retrieve /$container$/partials",
                "input": "input",
                "instructions": [
                    "entitySet = retrieve input->container index input->index amount input->amount",
                    "output = from entitySet to data",
                    "return output"
                ],
                "view": "json"
            },
            "retrieve_set": {
                "pattern": "retrieve /$container$",
                "input": "input",
                "instructions": [
                    "entities = retrieve multiple input->container by uuid:input->uuids",
                    "output = from entities to data",
                    "return output"
                ],
                "view": "json"
            },
            "retrieve_by_keyname": {
                "pattern": "retrieve /$container$/$name$/$value",
                "input": "input",
                "instructions": [
                    "entity = retrieve input->container by input->name:input->value",
                    "output = from entity to data",
                    "return output"
                ],
                "view": "json"
            },
            "retrieve_by_uuid": {
                "pattern": "retrieve /$role|container$\/$[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}|uuid$",
                "input": "input",
                "instructions": [
                    "entity = retrieve input->container by uuid:input->uuid",
                    "output = from entity to data",
                    "return output"
                ],
                "view": "json"
            },
            "update": {
                "pattern": "update /$container$/$[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}|original_uuid$",
                "input": "input",
                "instructions": [
                    "original = retrieve input->container by uuid:input->uuid",
                    "updated = from input to input->container",
                    "update original using updated",
                    "output = from updated to data",
                    "return output"
                ],
                "view": "json"
            },
            "delete": {
                "pattern": "insert /$container$/$[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}|uuid$",
                "input": "input",
                "instructions": [
                    "entity = retrieve input->container by uuid:input->uuid",
                    "delete entity",
                    "output = from entity to data",
                    "return output"
                ],
                "view": "json"
            },
            "insert": {
                "pattern": "insert /$container$",
                "input": "input",
                "instructions": [
                    "entity = from input to input->container",
                    "insert entity",
                    "output = from entity to data",
                    "return output"
                ],
                "view": "json"
            },
            "insert_in_bulk": {
                "pattern": "insert /$container$/$name$/$value",
                "input": "input",
                "instructions": [
                    "entities = from input to multiple input->container",
                    "insert entities",
                    "output = from entities to data",
                    "return output"
                ],
                "view": "json"
            }
        },
